; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Viro"
run/main_scene="res://Scenes/General/main_terminal.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.4", "Forward Plus")
run/low_processor_mode=true
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Resources/icon.png"

[autoload]

Signals="*res://Scripts/System/signals.gd"
Audio="*res://Scripts/Audio/soundManager.gd"
Globals="*res://Scripts/System/globals.gd"
Tools="*res://Scripts/System/tools.gd"
System="*res://Scripts/System/system.gd"

[debug]

file_logging/log_path="user://logs/viro.log"
gdscript/warnings/unused_signal=0
gdscript/warnings/untyped_declaration=2
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://Resources/themes/viro_theme.tres"
theme/custom_font="res://Resources/themes/system_fonts.tres"
theme/default_font_multichannel_signed_distance_field=true

